{
    "name": "mediashare/spider",
    "description": "Spider is a php library for crawling website that allows you to scrape informations & automated actions.",
    "type": "vendor",
    "keywords": ["crawler","scrapper","library","scrappe", "modulable"],
    "homepage": "https://github.com/Mediashare/Spider",
    "license": "MIT",
    "authors": [
        {
            "name": "Thibault Marquand",
            "email": "Mediashare.supp@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "guzzlehttp/guzzle": "^6.3",
        "league/climate": "^3.5",
        "sensiolabs/ansi-to-html": "^1.1",
        "symfony/console": "^5.0",
        "symfony/css-selector": "5.0.*",
        "symfony/dom-crawler": "5.0.*",
        "symfony/dotenv": "^5.0",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "^5.0",
        "symfony/serializer-pack": "^1.0",
        "symfony/twig-pack": "^1.0",
        "symfony/validator": "^5.0",
        "symfony/yaml": "^5.0"
    },
    "require-dev": {
        "symfony/maker-bundle": "^1.12",
        "symfony/orm-pack": "^1.0",
        "symfony/profiler-pack": "^1.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Mediashare\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mediashare\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.0.*"
        }
    }
}
